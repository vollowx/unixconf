def_key "+"
  show_clock
def_key "="
  volume_up

def_key "j"
  scroll_down
def_key "k"
  scroll_up

def_key "ctrl-u"
  page_up
def_key "ctrl-d"
  page_down

def_key "g"
  move_home

def_key "G"
  move_end

def_key "h"
  previous_column
def_key "l"
  next_column

def_key "."
  show_lyrics

def_key "n"
  next_found_item
def_key "N"
  previous_found_item

def_key "J"
  move_selected_items_down
def_key "K"
  move_selected_items_up

def_key "d"
  delete_playlist_items
def_key "d"
  delete_browser_items
def_key "d"
  delete_stored_playlist
